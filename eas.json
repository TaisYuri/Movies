{
  "cli": {
    "version": ">= 2.1.0"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "buildConfiguration": "Debug"
      },
      "channel": "development",
      "env": {
        "api_key": "public",
        "API_KEY": "public",
        "access_token": "public",
        "ACCESS_TOKEN": "public"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "api_key": "public",
        "API_KEY": "public",
        "access_token": "public",
        "ACCESS_TOKEN": "public"
      }
    },
    "production": {
      "channel": "production",
      "env": {
        "api_key": "public",
        "API_KEY": "public",
        "access_token": "public",
        "ACCESS_TOKEN": "public"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
